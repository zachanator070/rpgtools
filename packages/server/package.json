{
  "name": "@rpgtools/server",
  "version": "4.1.0",
  "author": "zachanator070",
  "description": "Tool set for playing an RPG game",
  "license": "MIT",
  "repository": "https://github.com/zachanator070/rpgtools",
  "main": "src/electron/index.cjs",
  "type": "module",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node dist/server/src/index.js",
    "dev:start": "node --experimental-specifier-resolution=node --inspect=0.0.0.0 --enable-source-maps dist/server/src/index.js",
    "dev:start-brk": "node --experimental-specifier-resolution=node --inspect-brk=0.0.0.0 --enable-source-maps dist/server/src/index.js",
    "build": "tsc",
    "dev": "nodemon --watch src -e ts,ejs --exec npm-run-all build dev:start",
    "dev-brk": "nodemon --watch src -e ts,ejs --exec npm-run-all build dev:start-brk",
    "test:integration": "node --experimental-vm-modules ../../node_modules/jest/bin/jest --config jest.config.integration.cjs -i ${JEST_OPTIONS}",
    "test:unit": "node --experimental-vm-modules ../../node_modules/jest/bin/jest --config jest.config.unit.cjs",
    "preelectron": "cp -R ../frontend/dist dist/frontend",
    "electron": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "dependencies": {
    "@electron/remote": "^2.0.9",
    "@graphql-tools/schema": "^8.1.0",
    "@rpgtools/common": "*",
    "@types/bson": "^4.2.0",
    "apollo-server": "^3.1.2",
    "apollo-server-express": "^3.10.2",
    "archiver": "^5.3.0",
    "ascii-table": "^0.0.9",
    "async-mutex": "^0.3.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "express": "^4.17.1",
    "graphql-subscriptions": "^1.2.1",
    "graphql-tag": "^2.12.5",
    "graphql-upload": "^13.0.0",
    "graphql-ws": "^5.9.0",
    "gridfs-stream": "^1.1.1",
    "inversify": "^5.1.1",
    "jimp": "^0.16.1",
    "jsonwebtoken": "^9.0.0",
    "minimist": "^1.2.5",
    "mongoose": "^6.6.0",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "pg": "^8.8.0",
    "redis": "^4.3.1",
    "redis-rstream": "^1.0.1",
    "redis-wstream": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "remark-parse": "^10.0.0",
    "sequelize": "^6.29.0",
    "sqlite3": "^5.1.2",
    "umzug": "^3.2.1",
    "unified": "^10.1.0",
    "unist-util-visit": "^4.0.0",
    "unzipper": "^0.10.11",
    "uuid": "^8.3.2",
    "ws": "^8.12.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@electron-forge/cli": "^6.0.4",
    "@electron-forge/maker-deb": "^6.0.4",
    "@electron-forge/maker-dmg": "^6.0.4",
    "@electron-forge/maker-rpm": "^6.0.5",
    "@electron-forge/maker-squirrel": "^6.0.4",
    "@electron-forge/maker-zip": "^6.0.4",
    "@types/archiver": "^5.1.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/graphql-upload": "^8.0.11",
    "@types/jest": "^29.0.3",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/morgan": "^1.9.3",
    "@types/pg": "^8.6.5",
    "@types/redis": "^4.0.11",
    "@types/type-is": "^1.6.3",
    "@types/unzipper": "^0.10.4",
    "@types/uuid": "^8.3.1",
    "@types/ws": "^8.5.4",
    "babel-jest": "^29.0.1",
    "dotenv": "^16.0.3",
    "electron": "^22.1.0",
    "electron-builder-squirrel-windows": "^24.0.0-alpha.11",
    "electron-squirrel-startup": "^1.0.0",
    "eslint": "^8.23.0",
    "esm": "^3.2.25",
    "is-typedarray": "^1.0.0",
    "jest": "29.4.2",
    "lodash": "^4.17.21",
    "request": "^2.88.2",
    "ts-jest": "^29.0.1",
    "typedarray-to-buffer": "^4.0.0"
  }
}
